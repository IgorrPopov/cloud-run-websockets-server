{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-node",
        "request": "launch",
        "name": "staging",
        "args": [
            "${workspaceFolder}/src/main.ts"
        ],
        "runtimeArgs": [
            "--nolazy",
            "-r",
            "ts-node/register"
        ],
        "runtimeVersion": "10.15.3",
        "sourceMaps": true,
        "resolveSourceMapLocations": [
            "${workspaceFolder}/**",
            "!**/node_modules/**"
        ],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
      }
    ]
  }